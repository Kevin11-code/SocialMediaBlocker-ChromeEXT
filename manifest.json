{
  "manifest_version": 3,
  "name": "Cryptojacking Detection Master",
  "version": "0.1.1",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["Content.js"]
    }
  ],
  "icons": { "48": "logo.png", "128": "logo.png" },
  "action": {
    "default_popup": "html/popup.html",
    "default_title": "Cryptojacking Detection Master"
  },

  "permissions": ["storage"]
}
